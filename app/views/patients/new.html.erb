<h1>Patients#new</h1>
<p>Find me in app/views/patients/new.html.erb</p>
<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>New Form</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
</head>
<body>
  <div class="container">
    <h1>New Form</h1>
    
    <%= form_with(model: @form, local: true) do |form| %>
      <div class="form-group">
        <%= form.label :first_name %>
        <%= form.text_field :first_name, class: "form-control" %>
      </div>
      
      <div class="form-group">
        <%= form.label :last_name %>
        <%= form.text_field :last_name, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :gender %>
        <%= form.text_field :gender, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :date_of_birth %>
        <%= form.date_field :date_of_birth, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :address %>
        <%= form.text_field :address, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :relationship %>
        <%= form.text_field :relationship, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :hobbies %>
        <%= form.text_field :hobbies, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :height %>
        <%= form.number_field :height, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :weight %>
        <%= form.number_field :weight, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :conditions %>
        <%= form.text_field :conditions, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :medication %>
        <%= form.text_field :medication, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :services %>
        <%= form.text_field :services, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :start_date %>
        <%= form.date_field :start_date, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :end_date %>
        <%= form.date_field :end_date, class: "form-control" %>
      </div>

      <%= form.submit "Create Form", class: "btn btn-primary" %>
    <% end %>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>